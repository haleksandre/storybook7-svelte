{
  "name": "test",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "playwright test",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@playwright/test": "1.25.0",
    "@storybook/addon-essentials": "^7.0.0-alpha.54",
    "@storybook/addon-interactions": "^7.0.0-alpha.54",
    "@storybook/addon-links": "^7.0.0-alpha.54",
    "@storybook/addon-svelte-csf": "^3.0.0-next.0",
    "@storybook/svelte": "^7.0.0-alpha.54",
    "@storybook/svelte-vite": "^7.0.0-alpha.54",
    "@storybook/testing-library": "^0.0.13",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/package": "next",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-storybook": "^0.6.7",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "storybook": "^7.0.0-alpha.54",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0",
    "vitest": "^0.23.4"
  },
  "type": "module"
}
